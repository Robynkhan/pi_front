<div style="position:absolute;left:0;top:0;width:0;height:0;">
	<div style="position:absolute;left:0;top:0;width:100%;height:{{it.y}};"></div>
	<div style="position:absolute;left:0;top:{{it.y}};width:{{it.x}};height:{{it.h}};"></div>
	<div style="position:absolute;left:{{it.x+it.w}};top:{{it.y}};width:100%;height:{{it.h}};"></div>
	<div style="position:absolute;left:0;top:{{it.y+it.h}};width:100%;height:100%;"></div>

	<div style="position:absolute;left:{{it.x}};top:{{it.y}};width:{{it.w}};height:{{it.h}};pointer-events:none;"></div>
</div>